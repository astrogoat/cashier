{
    "name": "astrogoat/cashier",
    "description": "A Strata Wrapper App for Laravel Cashier",
    "keywords": [
        "astrogoat",
        "laravel",
        "strata",
        "cashier"
    ],
    "homepage": "https://github.com/astrogoat/cashier",
    "license": "MIT",
    "authors": [
        {
            "name": "Laura Tonning",
            "email": "tonning@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.hlx.dev"
        }
    ],
    "require": {
        "php": "^8.3",
        "astrogoat/strata": "^0.7.7|^0.8.0|^0.9.0",
        "illuminate/contracts": "^10.0",
        "laravel/cashier": "^v14.14.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3|^7.0",
        "orchestra/testbench": "^6.15|^8.5",
        "pestphp/pest": "^1.18|^2.16",
        "pestphp/pest-plugin-laravel": "^1.1|^2.2",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8|^5.15"
    },
    "autoload": {
        "psr-4": {
            "Astrogoat\\Cashier\\": "src",
            "Astrogoat\\Cashier\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrogoat\\Cashier\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "vaimo/composer-patches": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrogoat\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
